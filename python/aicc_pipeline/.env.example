# AICC Pipeline Configuration
# Copy this file to .env and modify as needed

# WebSocket URLs (모든 URL로 동시에 전송됨)
# Main WebSocket URL (required)
AICC_WS_URL=wss://hewable-wholly-wesley.ngrok-free.dev/api/v1/agent/check

# Additional WebSocket URLs (동시 전송용)
AICC_WS_URL_1=wss://hewable-wholly-wesley.ngrok-free.dev/api/v1/agent/check
# AICC_WS_URL_2=wss://OTHER_SERVER.ngrok-free.dev/api/v1/log

# UDP Ports
AICC_CUSTOMER_PORT=12345
AICC_AGENT_PORT=12346

# VAD Settings
AICC_VAD_THRESHOLD=0.5
AICC_MIN_SPEECH_MS=250.0
AICC_MIN_SILENCE_MS=400.0

# WebSocket Settings
AICC_WS_RECONNECT_INTERVAL=5.0
AICC_WS_QUEUE_MAXSIZE=1000

# STT Settings
AICC_STT_LANGUAGE=ko-KR
GOOGLE_APPLICATION_CREDENTIALS=/home/ubuntu/.config/gcloud/credentials.json

# Turn Detection Weights (must sum to 1.0)
AICC_TURN_MORPHEME_WEIGHT=0.6
AICC_TURN_DURATION_WEIGHT=0.2
AICC_TURN_SILENCE_WEIGHT=0.2
AICC_TURN_COMPLETE_THRESHOLD=0.65

# Debug Mode
AICC_DEBUG=false

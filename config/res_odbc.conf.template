; =============================================================================
; Asterisk ODBC Configuration
; /etc/asterisk/res_odbc.conf
; =============================================================================

[asterisk]
enabled => yes
dsn => asterisk-connector
username => admin
; password는 setup_odbc.sh에서 Secrets Manager로부터 설정됨
password => ${DB_PASSWORD}
pre-connect => yes
sanitysql => select 1
idlecheck => 60
connect_timeout => 10
